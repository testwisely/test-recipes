  before(:all) do    
    @browser = $browser = Selenium::WebDriver.for(browser_type)
    @browser.navigate.to(site_url)
  end

  after(:all) do
    @browser.quit unless debugging?
  end

  it "&lt;%= @info[:first_test_case] %&gt;" do
    # Test Steps go here
  end

end
